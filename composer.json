{
    "name": "laravue/laravue",
    "description": "A reasonable starting point for making single page webapps",
    "keywords": [
        "league",
        "laravue"
    ],
    "homepage": "https://github.com/lara-league/laravue",
    "license": "MIT",
    "authors": [
        {
            "name": "Russell Weas",
            "email": "russweas@gmail.com",
            "homepage": "rweas.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
